{
  "compilerOptions": {
    // "target": "invalid",
    // "lib": ["invalid-lib"],
    "allowJs": false,
    "checkJs": true,
    "strict": true,
    "noImplicitAny": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    // "jsx": "invalid",
    "incremental": true,
    "plugins": [
      {
        "name": "invalid-plugin"
      },
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ],
      "*": [
        "node_modules/*"
      ]
    },
    "baseUrl": "./",
    "typeRoots": [
      "./node_modules/@types"
    ],
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "skipLibCheck": true,
    "jsx": "preserve"
  },
  "include": [
    ".next/types/*/.ts",
    "src/*/",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "**/node_modules",
    "*/./",
    "./invalid"
  ]
}
